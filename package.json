{
    "name": "new-folder",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "yarn build && nodemon --exec ts-node -r tsconfig-paths/register dist/index.js",
        "build": "tsc",
        "docker:up": "docker-compose up --build",
        "docker:down": "docker-compose down",
        "docker:full": "yarn build && yarn docker:down && yarn docker:up",
        "docker:studio": "sudo docker exec -it onclick-server_onclick-app_1 bash -c \"npx prisma studio\"",
        "format": "npx prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
    },
    "main": "dist/index.js",
    "_moduleAliases": {
        "@*": "dist/*",
        "@controllers": "dist/controllers",
        "@middlewares": "dist/middlewares",
        "@validation": "dist/middlewares/validation",
        "@models": "dist/models",
        "@dao": "dist/models/dao",
        "@dto": "dist/models/dto",
        "@routes": "dist/routes",
        "@utilities": "dist/utilities",
        "@types": "dist/types"
    },
    "dependencies": {
        "@prisma/client": "5.12.1",
        "bcrypt": "^5.1.1",
        "blob-polyfill": "^7.0.20220408",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "cron": "^3.1.6",
        "cros": "^1.0.1",
        "dotenv": "^16.3.2",
        "express": "^4.18.2",
        "firebase": "^10.8.1",
        "firebase-admin": "^12.0.0",
        "http-errors": "^2.0.0",
        "module-alias": "^2.2.3",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.8",
        "pubsub-js": "^1.9.4",
        "redis": "^4.6.13",
        "tsc": "^2.0.4"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cookie-parser": "^1.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/handlebars": "^4.1.0",
        "@types/http-errors": "^2.0.4",
        "@types/joi": "^17.2.3",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/module-alias": "^2.0.4",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.11.6",
        "@types/nodemailer": "^6.4.14",
        "@types/redis": "^4.0.11",
        "jsonwebtoken": "^9.0.2",
        "nodemon": "^3.0.3",
        "prisma": "^5.12.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.3.3"
    }
}
